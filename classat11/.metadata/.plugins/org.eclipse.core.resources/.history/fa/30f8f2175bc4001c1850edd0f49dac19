package collectionVariables;

public class Sorting {
	public static void main(String[] args) {
		int[] num = {5,4,3,2,1};
		int temp;
		//bubble sort
		int i, j;
		for(j=0; j<4; j++) {
			if(num[j]>num[j+1]) {
				temp = num[j];
				num[j] = num[j+1];
				num[j+1] = temp;
			}
		}
		
		for(i=0; i<5; i++) {
			System.out.print(num[i]+"\t");
		}
		
	}
}
