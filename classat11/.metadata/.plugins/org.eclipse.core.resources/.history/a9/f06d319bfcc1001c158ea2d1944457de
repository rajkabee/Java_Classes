package basics.controlStatements.loops;

public class PrimeRange {
	public static void main(String[] args) {
		int i,num=79, flag=0;
		i=2;
		while(i<=(num/2)) {
			System.out.print(i+"\t");
			if(num%i==0) {
				//composite
				flag=1;
				break;
			}
			
			i++;
		}
		if(flag==0) {
			System.out.println("Prime");
		}else {
			System.out.println("Composite");
		}
	}
}
