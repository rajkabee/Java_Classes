package collectionVariables;

public class Sorting {
	public static void main(String[] args) {
		int[] num = {5,4,3,2,1};
		int temp;
		//bubble sort
		int i, j, k;
		for(i=0; i<4; i++) {
			for(j=0; j<4; j++) {
				if(num[j]>num[j+1]) {
					temp = num[j];
					num[j] = num[j+1];
					num[j+1] = temp;
				}
				for(k=0; k<5; k++) {
					System.out.print(num[k]+"\t");
				}
				System.out.println();
			}
		}
		
		
		
	}
}
